{
  "name": "github-repository-provider",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/github-provider.mjs",
  "module": "src/github-provider.mjs",
  "description": "repository provider for github",
  "keywords": [
    "git",
    "github",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/github-provider.mjs --section=API",
    "lint": "documentation lint src/github-provider.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^2.6.0",
    "@octokit/rest": "^16.29.0",
    "content-entry": "^2.2.2",
    "github-graphql-api": "^1.0.6",
    "micromatch": "^4.0.2",
    "repository-provider": "^16.7.2"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "repository-provider-test-support": "github:arlac77/repository-provider-test-support",
    "semantic-release": "^15.13.24"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/github-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/github-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/github-repository-provider#readme",
  "release": {},
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
