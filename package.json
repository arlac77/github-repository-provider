{
  "name": "github-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/github-provider.mjs",
  "module": "src/github-provider.mjs",
  "description": "repository provider for github",
  "keywords": [
    "git",
    "github",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/github-provider.mjs --section=API",
    "lint": "documentation lint src/github-provider.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^2.0.0",
    "@octokit/rest": "^16.3.2",
    "content-entry": "^2.0.2",
    "github-graphql-api": "^1.0.6",
    "micromatch": "^3.1.10",
    "repository-provider": "^11.0.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/github-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/github-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/github-repository-provider#readme",
  "release": {},
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
